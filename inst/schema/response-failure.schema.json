{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "status": {
            "enum": ["failure"]
        },
        "data": {
            "type": "null"
        },
        "errors": {
            "type": "array",
            "items": {
                "$ref": "error-detail.schema.json"
            }
        }
    },
    "required": ["status", "data", "errors"],
    "additionalProperties": true
}
